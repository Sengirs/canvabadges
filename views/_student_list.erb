<% if @badge_placement_config && (@badge_placement_config.configured? || @badge_placement_config.award_only?) %>
  <ul class='nav nav-pills' style='margin-bottom: 5px; margin-top: 25px;'>
    <li id='current_students'>
      <a href="#"><%= I18n.t("partials.student_list.current_students")%></a>
    </li><li id='awarded_students'>
      <a href="#"><%= I18n.t("partials.student_list.awarded_students")%></a>
    </li>
  </ul>
  <form class="form-inline" id="search_form">
    <input type="text" class="input-large" placeholder="Search Users" title="search" id="search"/>
    <button class="btn btn-primary" type="submit"><%= I18n.t('partials.student_list.go')%></button>
    <button class="btn" type="button" id="clear_search"><%= I18n.t('partials.student_list.clear') %></button>
  </form>
  <table id="badges" class="table table-bordered table-striped" style='margin: 0 0 15px 0;' data-badge_config_id="<%= @badge_config_id %>" data-badge_placement_config_id="<%= @badge_placement_config_id %>">
    <thead>
      <tr>
        <th><%= I18n.t('partials.student_list.student')%></th>
        <th><%= I18n.t('partials.student_list.earned')%></th>
        <th><%= I18n.t('partials.student_list.issued')%></th>
    </thead>
    <tbody>
    </tbody>
  </table>
<% end %>
